    "contentVersion": "1.0.0.0",
    "parameters": {
      "principalId": {
        "value": "${AZURE_PRINCIPAL_ID}"
      },
      "ReportingDb": {
        "value": "${AZURE_REPORTING_DB}"
      },
      "AZURE_OPENAI_ENDPOINT": {
